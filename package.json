{
  "name": "shaojiang.io",
  "version": "0.0.1",
  "description": "A redux boilerplate with TypeScript",
  "repository": "https://github.com/agongdai/shaojiang.io.git",
  "scripts": {
    "start": "yarn run start:local",
    "start:dev": "cross-env NODE_ENV=dev node --max_old_space_size=4096 ./node_modules/webpack-dashboard/bin/webpack-dashboard.js -- webpack-dev-server --config ./webpack.config.js",
    "start:local": "cross-env NODE_ENV=local webpack-dashboard -- webpack-dev-server --config ./webpack.config.js",
    "start:prod": "cross-env NODE_ENV=production webpack-dashboard -- webpack-dev-server --config ./webpack.config.js",
    "build:local": "cross-env NODE_ENV=local webpack --env.patch --config ./webpack.config.js",
    "build:dev": "cross-env NODE_ENV=dev webpack --env.patch --config ./webpack/config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --env.patch --config ./webpack.config.js -p",
    "build": "yarn run build:prod",
    "test": "jest -u",
    "test:watch": "yarn run test -- --watch",
    "start:nodash": "webpack-dev-server --config ./webpack.config.js",
    "precommit": "lint-staged",
    "lint-js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix-js": "yarn run lint-js --fix",
    "fix-css": "yarn run lint-css --fix",
    "fix": "yarn run fix-js && yarn run fix-css"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "prettier --write",
      "git add --force"
    ],
    "*.{css,less,styl,scss,sass,sss}": [
      "stylelint --fix",
      "git add --force"
    ]
  },
  "author": "Shaojiang Cai <caishaojiang@gmail.com>",
  "license": "",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.5",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.3",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "inline-manifest-webpack-plugin": "^4.0.2",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss": "^7.0.13",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/pro-light-svg-icons": "^5.6.3",
    "@fortawesome/pro-regular-svg-icons": "^5.6.3",
    "@fortawesome/pro-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.2",
    "aws-amplify": "^1.1.19",
    "aws-amplify-react": "^2.3.0",
    "aws-sdk": "^2.391.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "dotenv-webpack": "^1.7.0",
    "i18next": "^13.1.4",
    "i18next-browser-languagedetector": "^2.2.4",
    "js-base64": "^2.5.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-ga": "^2.5.6",
    "react-helmet-async": "^0.2.0",
    "react-hot-loader": "^4.6.3",
    "react-hotkeys": "^1.1.4",
    "react-i18next": "^9.0.4",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-thunk": "^1.0.0",
    "react-toastify": "^4.5.2",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "validator": "^10.11.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/store/configure-store.js",
      "<rootDir>/jest/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/jest/coverage/",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/jest/__mocks__/styleMock.js",
      "Models/(.*)": "<rootDir>/src/models/$1",
      "Components/(.*)": "<rootDir>/src/components/$1",
      "Utils/(.*)": "<rootDir>/src/utils/$1",
      "Constants/(.*)": "<rootDir>/src/constants/$1",
      "Elements/(.*)": "<rootDir>/src/elements/$1",
      "Reducers/(.*)": "<rootDir>/src/reducers/$1",
      "Store/(.*)": "<rootDir>/src/store/$1",
      "Styles/(.*)": "<rootDir>/src/styles/$1",
      "Partials/(.*)": "<rootDir>/src/partials/$1",
      "Configs/(.*)": "<rootDir>/src/config/$1",
      "Root/(.*)": "<rootDir>/$1"
    },
    "setupFiles": [
      "<rootDir>/jest/shims.ts"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest/setup.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)?$"
  }
}
